    "erm": "0.0.1",
    "maki": "FabricLabs/maki#fabric"
  },
  "keywords": [
    "slack",
    "bot",
    "chat",
    "chatbot",
    "triggers",
    "ai",
    "machine learning",
    "snarl",
    "doorman"
  ],
  "licenses": {
    "type": "MIT",
    "url": "http://www.opensource.org/licenses/MIT"
  },
  "bugs": {
    "url": "https://github.com/martindale/snarl/issues"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "http-server": "^0.11.1",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0"
  },
  "preferGlobal": true,
  "scripts": {
    "docs": "npm run make:docs && http-server docs/",
