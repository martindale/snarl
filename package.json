  "name": "snarl",
  "description": "Friendly chatbot for soundtrack.io and more.  Powered by `doorman`.",
  "homepage": "https://github.com/martindale/snarl",
  "version": "0.4.0-pre",
  "repository": {
    "type": "git",
    "url": "https://github.com/martindale/snarl"
  },
  "main": "lib/snarl.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
